<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chocopi.jeonjucycle">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:icon="@drawable/jeonjucycle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.chocopi.jeonjucycle.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chocopi.jeonjucycle.FragmentTheme" />
        <activity
            android:name="com.chocopi.jeonjucycle.popupActivity"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog">
            <intent-filter>
                <action android:name="org.neulboworld.airmouse.intentfilter.speedpopup" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.chocopi.jeonjucycle.ThemeActivity"/>
        <activity android:name="com.chocopi.jeonjucycle.ThemeActivity2"/>
        <activity android:name="com.chocopi.jeonjucycle.Course1" />
        <activity android:name="com.chocopi.jeonjucycle.Course2" />
        <activity android:name="com.chocopi.jeonjucycle.Course3" />
        <activity android:name="com.chocopi.jeonjucycle.Course4" />
        <activity android:name="com.chocopi.jeonjucycle.Course5" />
        <activity android:name="com.chocopi.jeonjucycle.Course6" />
        <activity android:name="com.chocopi.jeonjucycle.MapActivity" />
    </application>

</manifest>